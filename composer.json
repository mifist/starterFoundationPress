{
    "name": "mifist/starterfoundationpress",
    "description": "starterFoundationPress is a WordPress starter theme",
    "version": "1.0.0",
    "type": "wordpress-theme",
    "config": {
        "vendor-dir": "vendor"
    },
    "minimum-stability" : "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Ole Fredrik Lie",
            "email": "mail@olefredrik.com"
        },
        {
            "name": "Simon Mayerhofer",
            "email": "foundationpress@maysi.de"
        },
        {
            "name": "Mifist",
            "email": "sg.d.moskalets@gmail.com",
            "homepage": "https://github.com/mifist/diviCreativeAgency",
            "role": "Developer"
        }
    ],
    "require": {
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.5.5",
        "wp-coding-standards/wpcs": "2.3.0"
    },
    "autoload": {
        "psr-4": {
            "starterFoundationPress\\": "inc/"
        },
        "classmap": ["inc/"]
    },
    "scripts": {
        "phpcs-config": [
            "\"vendor/bin/phpcs\" --config-set installed_paths ../../wp-coding-standards/wpcs",
            "\"vendor/bin/phpcs\" --config-set ignore_warnings_on_exit 1"
        ],
        "post-install-cmd": ["@phpcs-config"],
        "post-update-cmd": ["@phpcs-config"]
    }
}
